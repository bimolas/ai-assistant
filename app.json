{
  "expo": {
    "name": "YoRHa 2B",
    "slug": "2B",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/yorha-logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/yorha-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#B0AB98"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yorha.2b"
    },
    "android": {
      "package": "com.yorha.app2b",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/yorha-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "QUERY_ALL_PACKAGES",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/yorha-logo.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow YoRHa 2B to access your microphone for voice recording."
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "4e22e0cb-e267-4ea1-ba77-e3a3a839f71d"
      }
    }
  }
}